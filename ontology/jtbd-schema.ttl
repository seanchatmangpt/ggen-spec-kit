# ============================================================================
# SHACL VALIDATION SHAPES
# ============================================================================

# ----------------------------------------------------------------------------
# Job Shape
# ----------------------------------------------------------------------------

jtbd:JobShape a sh:NodeShape ;
    sh:targetClass jtbd:Job ;
    sh:property [
        sh:path jtbd:jobTitle ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minLength 5 ;
        sh:maxLength 100 ;
        sh:message "Job must have title (5-100 characters)"@en ;
    ] ;
    sh:property [
        sh:path jtbd:jobDescription ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minLength 20 ;
        sh:message "Job description required (at least 20 characters)"@en ;
    ] ;
    sh:property [
        sh:path jtbd:jobType ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:in ( "Functional" "Emotional" "Social" ) ;
        sh:message "Job type must be Functional, Emotional, or Social"@en ;
    ] ;
    sh:property [
        sh:path jtbd:hasJobStatement ;
        sh:class jtbd:JobStatement ;
        sh:minCount 1 ;
        sh:message "Job must have at least one job statement"@en ;
    ] ;
    sh:property [
        sh:path jtbd:hasDesiredOutcome ;
        sh:class jtbd:DesiredOutcome ;
        sh:minCount 1 ;
        sh:message "Job must have at least one desired outcome"@en ;
    ] .

# ----------------------------------------------------------------------------
# Job Statement Shape
# ----------------------------------------------------------------------------

jtbd:JobStatementShape a sh:NodeShape ;
    sh:targetClass jtbd:JobStatement ;
    sh:property [
        sh:path jtbd:when ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minLength 10 ;
        sh:message "Context (when) required (at least 10 characters)"@en ;
    ] ;
    sh:property [
        sh:path jtbd:wantTo ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minLength 10 ;
        sh:message "Action (want to) required (at least 10 characters)"@en ;
    ] ;
    sh:property [
        sh:path jtbd:soICan ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minLength 10 ;
        sh:message "Benefit (so I can) required (at least 10 characters)"@en ;
    ] ;
    sh:property [
        sh:path jtbd:fullStatement ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minLength 30 ;
        sh:message "Full job statement required (at least 30 characters)"@en ;
    ] .

# ----------------------------------------------------------------------------
# Desired Outcome Shape
# ----------------------------------------------------------------------------

jtbd:DesiredOutcomeShape a sh:NodeShape ;
    sh:targetClass jtbd:DesiredOutcome ;
    sh:property [
        sh:path jtbd:outcomeStatement ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minLength 15 ;
        sh:message "Outcome statement required (at least 15 characters)"@en ;
    ] ;
    sh:property [
        sh:path jtbd:outcomeDirection ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:in ( "Minimize" "Maximize" ) ;
        sh:message "Outcome direction must be Minimize or Maximize"@en ;
    ] ;
    sh:property [
        sh:path jtbd:outcomeMetric ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Outcome metric required (e.g., time, likelihood, number)"@en ;
    ] ;
    sh:property [
        sh:path jtbd:outcomeObject ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Outcome object required (what is being measured)"@en ;
    ] .

# ----------------------------------------------------------------------------
# Customer Segment Shape
# ----------------------------------------------------------------------------

jtbd:CustomerSegmentShape a sh:NodeShape ;
    sh:targetClass jtbd:CustomerSegment ;
    sh:property [
        sh:path jtbd:segmentName ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minLength 5 ;
        sh:maxLength 100 ;
        sh:message "Segment name required (5-100 characters)"@en ;
    ] ;
    sh:property [
        sh:path jtbd:segmentDescription ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minLength 30 ;
        sh:message "Segment description required (at least 30 characters)"@en ;
    ] ;
    sh:property [
        sh:path jtbd:hasPersona ;
        sh:class jtbd:Persona ;
        sh:minCount 1 ;
        sh:message "Customer segment must have at least one persona"@en ;
    ] .

# ----------------------------------------------------------------------------
# Persona Shape
# ----------------------------------------------------------------------------

jtbd:PersonaShape a sh:NodeShape ;
    sh:targetClass jtbd:Persona ;
    sh:property [
        sh:path jtbd:personaName ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minLength 3 ;
        sh:maxLength 50 ;
        sh:message "Persona name required (3-50 characters)"@en ;
    ] ;
    sh:property [
        sh:path jtbd:personaRole ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Persona role required"@en ;
    ] ;
    sh:property [
        sh:path jtbd:personaGoals ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:minLength 20 ;
        sh:message "Persona goals required (at least 20 characters)"@en ;
    ] ;
    sh:property [
        sh:path jtbd:personaChallenges ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:minLength 20 ;
        sh:message "Persona challenges required (at least 20 characters)"@en ;
    ] .

# ----------------------------------------------------------------------------
# Painpoint Shape
# ----------------------------------------------------------------------------

jtbd:PainpointShape a sh:NodeShape ;
    sh:targetClass jtbd:Painpoint ;
    sh:property [
        sh:path jtbd:painpointDescription ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minLength 15 ;
        sh:message "Painpoint description required (at least 15 characters)"@en ;
    ] ;
    sh:property [
        sh:path jtbd:painpointSeverity ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:in ( "Minor" "Moderate" "Critical" ) ;
        sh:message "Painpoint severity must be Minor, Moderate, or Critical"@en ;
    ] .

# ----------------------------------------------------------------------------
# Success Metric Shape
# ----------------------------------------------------------------------------

jtbd:SuccessMetricShape a sh:NodeShape ;
    sh:targetClass jtbd:SuccessMetric ;
    sh:property [
        sh:path jtbd:metricName ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minLength 5 ;
        sh:message "Metric name required (at least 5 characters)"@en ;
    ] ;
    sh:property [
        sh:path jtbd:metricType ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:in ( "Time" "Quality" "Effort" "Cost" "Throughput" ) ;
        sh:message "Metric type must be Time, Quality, Effort, Cost, or Throughput"@en ;
    ] ;
    sh:property [
        sh:path jtbd:targetValue ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Target value required for success metric"@en ;
    ] .

# ----------------------------------------------------------------------------
# Opportunity Score Shape
# ----------------------------------------------------------------------------

jtbd:OpportunityScoreShape a sh:NodeShape ;
    sh:targetClass jtbd:OpportunityScore ;
    sh:property [
        sh:path jtbd:opportunityScoreValue ;
        sh:datatype xsd:decimal ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minInclusive 0.0 ;
        sh:maxInclusive 30.0 ;
        sh:message "Opportunity score must be between 0 and 30"@en ;
    ] ;
    sh:property [
        sh:path jtbd:opportunityType ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:in ( "Overserved" "Appropriately Served" "Underserved" "Severely Underserved" ) ;
        sh:message "Opportunity type must be Overserved, Appropriately Served, Underserved, or Severely Underserved"@en ;
    ] .

# ----------------------------------------------------------------------------
# Outcome Priority Shape
# ----------------------------------------------------------------------------

jtbd:OutcomePriorityShape a sh:NodeShape ;
    sh:targetClass jtbd:OutcomePriority ;
    sh:property [
        sh:path jtbd:importanceScore ;
        sh:datatype xsd:decimal ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minInclusive 1.0 ;
        sh:maxInclusive 10.0 ;
        sh:message "Importance score must be between 1 and 10"@en ;
    ] ;
    sh:property [
        sh:path jtbd:satisfactionScore ;
        sh:datatype xsd:decimal ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minInclusive 1.0 ;
        sh:maxInclusive 10.0 ;
        sh:message "Satisfaction score must be between 1 and 10"@en ;
    ] .

