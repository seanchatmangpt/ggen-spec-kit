@prefix sk: <http://github.com/github/spec-kit#> .
@prefix jtbd: <http://github.com/github/spec-kit/jtbd#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# ============================================================================
# Jobs-to-be-Done Customer Jobs and Outcomes for Spec-Kit
# ============================================================================
# Defines concrete customer jobs and desired outcomes using JTBD framework
# for four primary personas using the spec-kit RDF-first development toolkit
# ============================================================================

jtbd:JTBDSpecification
  a sk:Documentation ;
  rdfs:label "Jobs-to-be-Done Customer Analysis" ;
  sk:documentTitle "Jobs-to-be-Done: Customer Jobs and Outcomes" ;
  sk:documentDescription "Concrete customer jobs, outcomes, painpoints, and progress makers for spec-kit personas using JTBD framework"@en ;
  sk:documentVersion "1.0" ;
  sk:createdDate "2025-12-21T00:00:00Z"^^xsd:dateTime ;
  sk:authorName "Spec-Kit Team" ;
  sk:maintenanceStatus "Current" ;
  sk:audience "Product teams, UX designers, and developers optimizing spec-kit experience"@en ;
  sk:category "Customer Research" ;
  .

# ============================================================================
# JTBD ONTOLOGY CLASSES
# ============================================================================

jtbd:Persona a owl:Class ;
  rdfs:label "Persona" ;
  rdfs:comment "User archetype with specific jobs and goals using spec-kit" .

jtbd:FunctionalJob a owl:Class ;
  rdfs:label "Functional Job" ;
  rdfs:comment "Concrete task the persona is trying to accomplish" .

jtbd:EmotionalJob a owl:Class ;
  rdfs:label "Emotional Job" ;
  rdfs:comment "How the persona wants to feel while using spec-kit" .

jtbd:SocialJob a owl:Class ;
  rdfs:label "Social Job" ;
  rdfs:comment "How the persona wants to be perceived by others" .

jtbd:Outcome a owl:Class ;
  rdfs:label "Desired Outcome" ;
  rdfs:comment "Specific measurable outcome the persona wants to achieve" .

jtbd:Painpoint a owl:Class ;
  rdfs:label "Painpoint" ;
  rdfs:comment "Current friction preventing job completion" .

jtbd:ProgressMaker a owl:Class ;
  rdfs:label "Progress Maker" ;
  rdfs:comment "Force pushing toward spec-kit adoption" .

# ============================================================================
# JTBD PROPERTIES
# ============================================================================

jtbd:personaName a owl:DatatypeProperty ;
  rdfs:domain jtbd:Persona ;
  rdfs:range xsd:string ;
  rdfs:comment "Name of the persona" .

jtbd:personaDescription a owl:DatatypeProperty ;
  rdfs:domain jtbd:Persona ;
  rdfs:range xsd:string ;
  rdfs:comment "Description of the persona's role and context" .

jtbd:jobStatement a owl:DatatypeProperty ;
  rdfs:domain jtbd:FunctionalJob ;
  rdfs:range xsd:string ;
  rdfs:comment "Job statement in format: When [situation], I want to [motivation], so I can [outcome]" .

jtbd:jobDescription a owl:DatatypeProperty ;
  rdfs:range xsd:string ;
  rdfs:comment "Detailed description of the job" .

jtbd:outcomeStatement a owl:DatatypeProperty ;
  rdfs:domain jtbd:Outcome ;
  rdfs:range xsd:string ;
  rdfs:comment "Measurable outcome statement" .

jtbd:outcomeMetric a owl:DatatypeProperty ;
  rdfs:domain jtbd:Outcome ;
  rdfs:range xsd:string ;
  rdfs:comment "Measurement dimension (time, quality, reliability, etc.)" .

jtbd:currentBaseline a owl:DatatypeProperty ;
  rdfs:domain jtbd:Outcome ;
  rdfs:range xsd:string ;
  rdfs:comment "Current performance baseline" .

jtbd:targetValue a owl:DatatypeProperty ;
  rdfs:domain jtbd:Outcome ;
  rdfs:range xsd:string ;
  rdfs:comment "Target performance value" .

jtbd:painpointDescription a owl:DatatypeProperty ;
  rdfs:domain jtbd:Painpoint ;
  rdfs:range xsd:string ;
  rdfs:comment "Description of current friction or pain" .

jtbd:impactLevel a owl:DatatypeProperty ;
  rdfs:range xsd:string ;
  rdfs:comment "Impact severity: Critical, High, Medium, Low" .

jtbd:progressMakerDescription a owl:DatatypeProperty ;
  rdfs:domain jtbd:ProgressMaker ;
  rdfs:range xsd:string ;
  rdfs:comment "Description of force pushing toward adoption" .

jtbd:progressMakerCategory a owl:DatatypeProperty ;
  rdfs:domain jtbd:ProgressMaker ;
  rdfs:range xsd:string ;
  rdfs:comment "Category: Time Savings, Quality Improvement, Reduced Cognitive Load, etc." .

# JTBD relationships
jtbd:hasFunctionalJob a owl:ObjectProperty ;
  rdfs:domain jtbd:Persona ;
  rdfs:range jtbd:FunctionalJob .

jtbd:hasEmotionalJob a owl:ObjectProperty ;
  rdfs:domain jtbd:Persona ;
  rdfs:range jtbd:EmotionalJob .

jtbd:hasSocialJob a owl:ObjectProperty ;
  rdfs:domain jtbd:Persona ;
  rdfs:range jtbd:SocialJob .

jtbd:hasOutcome a owl:ObjectProperty ;
  rdfs:domain jtbd:Persona ;
  rdfs:range jtbd:Outcome .

jtbd:hasPainpoint a owl:ObjectProperty ;
  rdfs:domain jtbd:Persona ;
  rdfs:range jtbd:Painpoint .

jtbd:hasProgressMaker a owl:ObjectProperty ;
  rdfs:domain jtbd:Persona ;
  rdfs:range jtbd:ProgressMaker .

# ============================================================================
# PERSONA 1: RDF ONTOLOGY DESIGNER
# ============================================================================

jtbd:Persona_OntologyDesigner
  a jtbd:Persona ;
  jtbd:personaName "RDF Ontology Designer" ;
  jtbd:personaDescription "Creates semantic specifications from user requirements and domain knowledge. Translates business needs into RDF ontologies that serve as source code. Works with SHACL shapes, SPARQL queries, and Tera templates to define the μ transformation pipeline." ;
  jtbd:hasFunctionalJob jtbd:Job_OD_ModelDomainKnowledge ;
  jtbd:hasFunctionalJob jtbd:Job_OD_ValidateSpecifications ;
  jtbd:hasEmotionalJob jtbd:EmotionalJob_OD_ConfidenceInCompleteness ;
  jtbd:hasEmotionalJob jtbd:EmotionalJob_OD_SatisfactionInElegance ;
  jtbd:hasSocialJob jtbd:SocialJob_OD_SemanticExpert ;
  jtbd:hasOutcome jtbd:Outcome_OD_1 ;
  jtbd:hasOutcome jtbd:Outcome_OD_2 ;
  jtbd:hasOutcome jtbd:Outcome_OD_3 ;
  jtbd:hasOutcome jtbd:Outcome_OD_4 ;
  jtbd:hasOutcome jtbd:Outcome_OD_5 ;
  jtbd:hasOutcome jtbd:Outcome_OD_6 ;
  jtbd:hasOutcome jtbd:Outcome_OD_7 ;
  jtbd:hasPainpoint jtbd:Pain_OD_1 ;
  jtbd:hasPainpoint jtbd:Pain_OD_2 ;
  jtbd:hasPainpoint jtbd:Pain_OD_3 ;
  jtbd:hasPainpoint jtbd:Pain_OD_4 ;
  jtbd:hasPainpoint jtbd:Pain_OD_5 ;
  jtbd:hasProgressMaker jtbd:Progress_OD_1 ;
  jtbd:hasProgressMaker jtbd:Progress_OD_2 ;
  jtbd:hasProgressMaker jtbd:Progress_OD_3 ;
  .

# Functional Jobs - Ontology Designer
jtbd:Job_OD_ModelDomainKnowledge
  a jtbd:FunctionalJob ;
  jtbd:jobStatement "When requirements arrive, I want to model domain knowledge in RDF, so I can create a single source of truth that compiles to code" ;
  jtbd:jobDescription "Transform user requirements and domain understanding into precise RDF ontologies with classes, properties, SHACL shapes, and semantic constraints. Create specifications that are executable - deterministic enough to generate working implementations across multiple languages." ;
  .

jtbd:Job_OD_ValidateSpecifications
  a jtbd:FunctionalJob ;
  jtbd:jobStatement "When I've created RDF specifications, I want to validate semantic correctness and completeness, so I can catch errors before code generation" ;
  jtbd:jobDescription "Validate RDF syntax, SHACL shape constraints, SPARQL query correctness, and template rendering before committing specifications. Ensure ontologies are internally consistent and capture all requirements." ;
  .

# Emotional Jobs - Ontology Designer
jtbd:EmotionalJob_OD_ConfidenceInCompleteness
  a jtbd:EmotionalJob ;
  jtbd:jobDescription "Feel confident that my RDF specifications are complete, unambiguous, and will generate correct implementations without surprises" ;
  .

jtbd:EmotionalJob_OD_SatisfactionInElegance
  a jtbd:EmotionalJob ;
  jtbd:jobDescription "Feel satisfaction in creating elegant semantic models that are both machine-executable and human-readable" ;
  .

# Social Jobs - Ontology Designer
jtbd:SocialJob_OD_SemanticExpert
  a jtbd:SocialJob ;
  jtbd:jobDescription "Be perceived as a semantic modeling expert who delivers precise, high-quality specifications that development teams trust" ;
  .

# Outcomes - Ontology Designer
jtbd:Outcome_OD_1
  a jtbd:Outcome ;
  jtbd:outcomeStatement "Minimize time to create validated RDF specification from requirements" ;
  jtbd:outcomeMetric "Time" ;
  jtbd:currentBaseline "4-6 hours to model a feature domain in RDF" ;
  jtbd:targetValue "1-2 hours with AI-assisted modeling and real-time SHACL validation" ;
  .

jtbd:Outcome_OD_2
  a jtbd:Outcome ;
  jtbd:outcomeStatement "Maximize semantic correctness of RDF specifications before code generation" ;
  jtbd:outcomeMetric "Quality" ;
  jtbd:currentBaseline "2-3 SHACL validation errors discovered during ggen sync" ;
  jtbd:targetValue "0 validation errors - catch all issues during RDF authoring" ;
  .

jtbd:Outcome_OD_3
  a jtbd:Outcome ;
  jtbd:outcomeStatement "Minimize ambiguity in ontology definitions that lead to incorrect code generation" ;
  jtbd:outcomeMetric "Quality" ;
  jtbd:currentBaseline "15-20% of generated code requires manual fixes due to spec ambiguity" ;
  jtbd:targetValue "< 5% manual fixes - specifications precise enough for deterministic generation" ;
  .

jtbd:Outcome_OD_4
  a jtbd:Outcome ;
  jtbd:outcomeStatement "Maximize reusability of ontology patterns across features" ;
  jtbd:outcomeMetric "Efficiency" ;
  jtbd:currentBaseline "30% code reuse across features (copy-paste RDF patterns)" ;
  jtbd:targetValue "70% reuse via shared ontology modules and pattern libraries" ;
  .

jtbd:Outcome_OD_5
  a jtbd:Outcome ;
  jtbd:outcomeStatement "Minimize effort to evolve ontology when requirements change" ;
  jtbd:outcomeMetric "Time" ;
  jtbd:currentBaseline "2-3 hours to update RDF and regenerate all affected artifacts" ;
  jtbd:targetValue "< 30 minutes with incremental ggen sync and impact analysis" ;
  .

jtbd:Outcome_OD_6
  a jtbd:Outcome ;
  jtbd:outcomeStatement "Maximize confidence that RDF specifications capture all edge cases" ;
  jtbd:outcomeMetric "Reliability" ;
  jtbd:currentBaseline "Edge cases discovered during testing (40% coverage in specs)" ;
  jtbd:targetValue "90% edge case coverage captured in SHACL shapes and constraints" ;
  .

jtbd:Outcome_OD_7
  a jtbd:Outcome ;
  jtbd:outcomeStatement "Minimize learning curve for team members to read and understand RDF specifications" ;
  jtbd:outcomeMetric "Maintainability" ;
  jtbd:currentBaseline "2-3 days for developer to understand RDF ontology structure" ;
  jtbd:targetValue "< 4 hours with generated documentation and interactive examples" ;
  .

# Painpoints - Ontology Designer
jtbd:Pain_OD_1
  a jtbd:Painpoint ;
  jtbd:painpointDescription "SHACL validation errors only appear when running ggen sync, not during RDF authoring - leads to long feedback loops" ;
  jtbd:impactLevel "High" ;
  .

jtbd:Pain_OD_2
  a jtbd:Painpoint ;
  jtbd:painpointDescription "No tooling to visualize ontology structure and relationships - must mentally parse Turtle syntax" ;
  jtbd:impactLevel "Medium" ;
  .

jtbd:Pain_OD_3
  a jtbd:Painpoint ;
  jtbd:painpointDescription "Difficult to track which parts of RDF specification are actually used in templates vs. unused definitions" ;
  jtbd:impactLevel "Medium" ;
  .

jtbd:Pain_OD_4
  a jtbd:Painpoint ;
  jtbd:painpointDescription "No automated testing for SPARQL queries - must manually verify query results match expected data" ;
  jtbd:impactLevel "High" ;
  .

jtbd:Pain_OD_5
  a jtbd:Painpoint ;
  jtbd:painpointDescription "Breaking changes to ontology structure require manual updates across SPARQL queries and Tera templates" ;
  jtbd:impactLevel "Critical" ;
  .

# Progress Makers - Ontology Designer
jtbd:Progress_OD_1
  a jtbd:ProgressMaker ;
  jtbd:progressMakerDescription "Deterministic code generation eliminates 'specification drift' - generated code always matches RDF source" ;
  jtbd:progressMakerCategory "Quality Improvement" ;
  .

jtbd:Progress_OD_2
  a jtbd:ProgressMaker ;
  jtbd:progressMakerDescription "Single RDF specification compiles to multiple languages (Python, TypeScript, Rust, Java, C#, Go) - write once, deploy everywhere" ;
  jtbd:progressMakerCategory "Efficiency" ;
  .

jtbd:Progress_OD_3
  a jtbd:ProgressMaker ;
  jtbd:progressMakerDescription "Semantic reasoning via SPARQL materializes implicit knowledge - reduces manual specification work" ;
  jtbd:progressMakerCategory "Reduced Cognitive Load" ;
  .

