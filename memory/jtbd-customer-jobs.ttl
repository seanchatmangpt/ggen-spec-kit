jtbd:Persona_CLIDeveloper
  a jtbd:Persona ;
  jtbd:personaName "CLI Developer" ;
  jtbd:personaDescription "Implements CLI commands based on RDF specifications following three-tier architecture (commands, ops, runtime). Writes business logic, subprocess execution, and telemetry instrumentation. Ensures specification-driven implementation matches generated types." ;
  jtbd:hasFunctionalJob jtbd:Job_CLI_ImplementCommands ;
  jtbd:hasFunctionalJob jtbd:Job_CLI_MaintainArchitecture ;
  jtbd:hasEmotionalJob jtbd:EmotionalJob_CLI_ConfidenceInCorrectness ;
  jtbd:hasEmotionalJob jtbd:EmotionalJob_CLI_PrideInCraftsmanship ;
  jtbd:hasSocialJob jtbd:SocialJob_CLI_SpecDrivenEngineer ;
  jtbd:hasOutcome jtbd:Outcome_CLI_1 ;
  jtbd:hasOutcome jtbd:Outcome_CLI_2 ;
  jtbd:hasOutcome jtbd:Outcome_CLI_3 ;
  jtbd:hasOutcome jtbd:Outcome_CLI_4 ;
  jtbd:hasOutcome jtbd:Outcome_CLI_5 ;
  jtbd:hasOutcome jtbd:Outcome_CLI_6 ;
  jtbd:hasOutcome jtbd:Outcome_CLI_7 ;
  jtbd:hasPainpoint jtbd:Pain_CLI_1 ;
  jtbd:hasPainpoint jtbd:Pain_CLI_2 ;
  jtbd:hasPainpoint jtbd:Pain_CLI_3 ;
  jtbd:hasPainpoint jtbd:Pain_CLI_4 ;
  jtbd:hasPainpoint jtbd:Pain_CLI_5 ;
  jtbd:hasProgressMaker jtbd:Progress_CLI_1 ;
