@prefix doc: <http://ggen-spec-kit.org/documentation#> .
@prefix spec: <http://ggen-spec-kit.org/spec#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

# ============================================================================
# DOCUMENTATION ONTOLOGY
# Self-describing specification for Spec Kit documentation
# ============================================================================

# Classes
doc:Documentation a rdfs:Class ;
    rdfs:label "Documentation" ;
    rdfs:comment "Any documentation artifact" .

doc:Tutorial rdfs:subClassOf doc:Documentation ;
    rdfs:label "Tutorial" ;
    rdfs:comment "Learning-oriented, hands-on guide for beginners" .

doc:HowToGuide rdfs:subClassOf doc:Documentation ;
    rdfs:label "How-To Guide" ;
    rdfs:comment "Task-oriented, goal-focused procedural guide" .

doc:Reference rdfs:subClassOf doc:Documentation ;
    rdfs:label "Reference" ;
    rdfs:comment "Information-oriented, lookup-based technical specification" .

doc:Explanation rdfs:subClassOf doc:Documentation ;
    rdfs:label "Explanation" ;
    rdfs:comment "Understanding-oriented, conceptual deep-dive" .

doc:ConceptualMap a rdfs:Class ;
    rdfs:label "Conceptual Map" ;
    rdfs:comment "Relationship graph between concepts" .

doc:Pattern a rdfs:Class ;
    rdfs:label "Pattern" ;
    rdfs:comment "Reusable pattern (few-shot example)" .

doc:CodeExample a rdfs:Class ;
    rdfs:label "Code Example" ;
    rdfs:comment "Working code example demonstrating a pattern" .

# Core Properties
doc:title a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range rdfs:Literal ;
    rdfs:label "Title" .

doc:summary a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range rdfs:Literal ;
    rdfs:label "Summary" ;
    rdfs:comment "Brief summary (< 200 words)" .

doc:content a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range rdfs:Literal ;
    rdfs:label "Content" .

doc:learningTime a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range xsd:duration ;
    rdfs:label "Learning Time" ;
    rdfs:comment "ISO 8601 duration (e.g., PT10M for 10 minutes)" .

doc:difficulty a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range [ rdf:type rdfs:Class ] ;
    rdfs:label "Difficulty Level" ;
    rdfs:comment "One of: Beginner, Intermediate, Advanced" .

doc:prerequisites a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range doc:Documentation ;
    rdfs:label "Prerequisites" ;
    rdfs:comment "What must be understood first" .

doc:relatedTo a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range doc:Documentation ;
    rdfs:label "Related To" ;
    rdfs:comment "Related documentation (non-hierarchical)" .

doc:linkedFrom a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range doc:Documentation ;
    rdfs:label "Linked From" ;
    rdfs:comment "Documentation that links to this" .

# Semantic Properties
doc:concept a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range rdfs:Resource ;
    rdfs:label "Core Concept" ;
    rdfs:comment "Central concept explained" .

doc:outcome a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range rdfs:Literal ;
    rdfs:label "Learning Outcome" ;
    rdfs:comment "What reader will know/be able to do" .

doc:pattern a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range doc:Pattern ;
    rdfs:label "Pattern" ;
    rdfs:comment "Reusable pattern demonstrated" .

doc:example a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range doc:CodeExample ;
    rdfs:label "Example" ;
    rdfs:comment "Working code example" .

# Verification Properties
doc:verifiedBy a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range spec:Specification ;
    rdfs:label "Verified By" ;
    rdfs:comment "Which specification verifies this documentation" .

doc:verificationStatus a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range [ rdf:type rdfs:Class ] ;
    rdfs:label "Verification Status" ;
    rdfs:comment "One of: Verified, Outdated, Unverified" .

doc:sha256 a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range rdfs:Literal ;
    rdfs:label "SHA256 Hash" ;
    rdfs:comment "Cryptographic proof of content" .

# Token Optimization
doc:tokenCount a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range xsd:integer ;
    rdfs:label "Token Count" ;
    rdfs:comment "Estimated tokens for this documentation" .

doc:compressedVersion a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range doc:Documentation ;
    rdfs:label "Compressed Version" ;
    rdfs:comment "Token-optimized version for AI" .

# File References
doc:markdownFile a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range rdfs:Resource ;
    rdfs:label "Markdown File" .

doc:jsonldFile a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range rdfs:Resource ;
    rdfs:label "JSON-LD File" .

doc:turtleFile a rdf:Property ;
    rdfs:domain doc:Documentation ;
    rdfs:range rdfs:Resource ;
    rdfs:label "Turtle File" .

