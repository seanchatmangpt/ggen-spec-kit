# ============================================================================
# CODE EXAMPLES
# ============================================================================

<http://ggen-spec-kit.org/example/hello-command> a doc:CodeExample ;
    rdfs:label "Hello Command Example" ;
    doc:content """
sk:hello
    a sk:Command ;
    rdfs:label "hello" ;
    sk:description "Greet the user" ;
    sk:hasModule "specify_cli.commands.hello" .
""" .

<http://ggen-spec-kit.org/example/greet-command> a doc:CodeExample ;
    rdfs:label "Greet Command with Argument" ;
    doc:content """
sk:greet
    a sk:Command ;
    rdfs:label "greet" ;
    sk:description "Greet a person by name" ;
    sk:hasModule "specify_cli.commands.greet" ;
    sk:hasArgument [
        a sk:Argument ;
        sk:name "name" ;
        sk:description "Name to greet" ;
        sk:required true ;
        sk:type "str"
    ] .
""" .

<http://ggen-spec-kit.org/example/impl-hello-operation> a doc:CodeExample ;
    rdfs:label "Hello Operation Implementation" ;
    doc:content """
def hello_operation() -> str:
    \"\"\"Generate greeting message.

    Returns
    -------
    str
        Greeting message
    \"\"\"
    return "Hello, World! Welcome to Spec Kit."
""" .

