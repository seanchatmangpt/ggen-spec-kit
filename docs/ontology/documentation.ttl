# ============================================================================
# SHACL SHAPES FOR VALIDATION
# ============================================================================

<http://ggen-spec-kit.org/shape/DocumentationShape> a sh:NodeShape ;
    sh:targetClass doc:Documentation ;
    sh:property [
        sh:path rdfs:label ;
        sh:minCount 1 ;
        sh:datatype rdfs:Literal ;
        sh:message "Every documentation must have a label"
    ] ;
    sh:property [
        sh:path doc:title ;
        sh:minCount 1 ;
        sh:datatype rdfs:Literal ;
        sh:message "Every documentation must have a title"
    ] ;
    sh:property [
        sh:path doc:summary ;
        sh:minCount 0 ;
        sh:maxLength 500 ;
        sh:message "Summary must be under 500 characters"
    ] ;
    sh:property [
        sh:path doc:difficulty ;
        sh:in ( "Beginner" "Intermediate" "Advanced" )
    ] .

<http://ggen-spec-kit.org/shape/TutorialShape> a sh:NodeShape ;
    sh:targetClass doc:Tutorial ;
    sh:property [
        sh:path doc:learningTime ;
        sh:minCount 1 ;
        sh:message "Tutorials must specify learning time"
    ] ;
    sh:property [
        sh:path doc:outcome ;
        sh:minCount 1 ;
        sh:message "Tutorials must specify learning outcomes"
    ] .

<http://ggen-spec-kit.org/shape/HowToGuideShape> a sh:NodeShape ;
    sh:targetClass doc:HowToGuide ;
    sh:property [
        sh:path doc:outcome ;
        sh:minCount 1 ;
        sh:message "How-to guides must specify the goal/outcome"
    ] .
